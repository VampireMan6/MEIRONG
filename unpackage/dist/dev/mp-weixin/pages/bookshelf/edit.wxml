<view><view class="cu-bar"><view class="action"><text data-event-opts="{{[['tap',[['clearClick',['$event']]]]]}}" bindtap="__e">清空全部</text></view><view class="action"><text data-event-opts="{{[['tap',[['BackClick',['$event']]]]]}}" bindtap="__e">完成</text></view></view><checkbox-group data-event-opts="{{[['change',[['CheckboxChange',['$event']]]]]}}" class="block" bindchange="__e"><block wx:for="{{DataList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="cu-form-group"><view class="title">{{item.name}}</view><checkbox class="{{['red',item.checked?'checked':'']}}" checked="{{item.checked?true:false}}" value="{{item.id}}"></checkbox></view></block></checkbox-group><view class="cu-tabbar-height margin-bottom"></view><block wx:if="{{delArr.length<=0}}"><view class="padding-left padding-right zai-tabbar-bar"><button class="cu-btn block line-red margin-tb-sm lg" disabled>删除选中</button></view></block><block wx:else><view class="padding-left padding-right flex flex-direction zai-tabbar-bar"><button data-event-opts="{{[['tap',[['DelClick',['$event']]]]]}}" class="cu-btn bg-red margin-tb-sm lg" bindtap="__e">{{"删除选中("+delArr.length+")"}}</button></view></block></view>